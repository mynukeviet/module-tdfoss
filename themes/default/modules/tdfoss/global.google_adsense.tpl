<!-- BEGIN: main -->
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- Block center -->
<ins class="adsbygoogle" <!-- BEGIN: size -->style="display:inline-block;width:{CONFIG.width}px;height:{CONFIG.height}px"<!-- END: size --> <!-- BEGIN: auto -->style="display: block"<!-- END: auto --> data-ad-client="{CONFIG.idclient}" data-ad-slot="{CONFIG.idslot}" data-ad-format="auto" data-full-width-responsive="true"></ins>
<script>
    (adsbygoogle = window.adsbygoogle || []).push({});
</script>
<!-- END: main -->

<!-- BEGIN: config -->
<tr>
    <td>{LANG.idclient}</td>
    <td><input type="text" name="config_idclient" class="form-control" value="{DATA.idclient}" placeholder="Ex: ca-pub-1191237483249432"></td>
</tr>
<tr>
    <td>{LANG.idslot}</td>
    <td><input type="text" name="config_idslot" class="form-control" value="{DATA.idslot}" placeholder="Ex: 6414843345"></td>
</tr>
<tr>
    <td>{LANG.size}</td>
    <td>
        <div class="row">
            <div class="col-xs-24 col-sm-12">
                <input type="text" name="config_width" class="form-control" value="{DATA.width}">
            </div>
            <div class="col-xs-24 col-sm-12">
                <input type="text" name="config_height" class="form-control" value="{DATA.height}">
            </div>
            <span class="help-block">{LANG.note}</span>
        </div>
    </td>
</tr>
<!-- END: config -->
